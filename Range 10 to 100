import java.util.Scanner;
public class Main{
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.print("Enter your number: ");
        int a = scan.nextInt();
        if (a >= 10 && a <= 100) {
            System.out.println("with in range");
        }
        else {
            System.out.println("invalid range");
        }
    }
}
